# SoloMethodEngine contract address (will be available after deployment)
SOLO_METHOD_ENGINE_ADDRESS=your_solo_method_engine_address_after_deployment

# Main distributor and treasury addresses 
MAIN_DISTRIBUTOR_ADDRESS=0xCe36333A88c2EA01f28f63131fA7dfa80AD021F6
TREASURY_ADDRESS=0x26A8401287cE33CC4aeb5a106cd6D282a9C2f51d

# Network configuration
NETWORK=polygon # Options: polygon, mumbai, hardhat, localhost

